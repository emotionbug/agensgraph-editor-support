lexer grammar AgensGraphLexer;

@members {
/* This field stores the tags which are used to detect the end of a dollar-quoted string literal.
*/
	  this._tags = [];
	  this.isLetter = (checkString) => {
        try {
            let regExSpecial = /[^\$_]/;
            return regExSpecial.test(checkString);
        } catch (error) {
            return false;
        }
	  };
}

